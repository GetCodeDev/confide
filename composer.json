{
    "name": "zizaco/confide",
    "type": "laravel-library",
    "description": "Confide is an authentication solution for Laravel 4",
    "keywords": ["laravel","illuminate","auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zizaco Zizuini",
            "email": "zizaco@gmail.com",
            "homepage": "http://www.zizaco.net"
        },
        {
            "name": "Andrew Elkins",
            "homepage": "http://www.andrewelkins.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "~4.1|^7",
        "composer/installers": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.8",
        "illuminate/auth": "~4.1|^7",
        "illuminate/database": "~4.1|^7"
    },
    "suggest": {
        "zizaco/entrust":"add Role-based Permissions to Laravel 4"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/commands"
        ],
        "psr-4": {
            "Zizaco\\Confide\\": "src/Zizaco/Confide/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zizaco\\Confide\\ConfideServiceProvider"
            ],
            "aliases":{
                "Confide": "Zizaco\\Confide\\ConfideFacade"
            }
        }
    },
    "minimum-stability": "stable"
}
